{
  "name": "natural-language-web",
  "version": "0.0.1",
  "description": "Interactive web platform for the Natural Language Project",
  "private": true,
  "main": "backend/dist/src/app.js",
  "scripts": {
    "start": "npm run serve",
    "start-dev": "npm run start-backend-dev && npm run start-frontend-dev",
    "start-frontend-dev": "node frontend/scripts/start.js",
    "start-backend-dev": "nodemon --watch backend/src backend/src/app.js --exec babel-node --presets es2015,stage-2",
    "build": "npm run build-backend && npm run build-frontend",
    "build-frontend": "node frontend/scripts/build.js",
    "build-backend": "babel backend/src -d dist && cp backend/src/api.yaml dist/",
    "serve": "node dist/app.js",
    "test": "mocha --compilers js:babel-register && node frontend/scripts/test.js --env=jsdom",
    "lint": "eslint frontend/src/js/**"
  },
  "author": "Gabriel Rodriguez and Mariano Furriel",
  "license": "ISC",
  "devDependencies": {
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "detect-port": "1.0.1",
    "enzyme": "^2.7.1",
    "http-proxy-middleware": "0.17.3",
    "image-webpack-loader": "^3.3.0",
    "jest": "18.1.0",
    "mocha": "^3.0.1",
    "nodemon": "^1.10.0",
    "object-assign": "4.1.1",
    "promise": "7.1.1",
    "react-addons-test-utils": "^15.4.2",
    "sinon": "^1.17.7",
    "webpack-dev-server": "1.16.2"
  },
  "dependencies": {
    "antd": "^2.8.2",
    "autoprefixer": "6.7.2",
    "axios": "^0.16.1",
    "babel-cli": "^6.11.4",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "^6.24.1",
    "babel-plugin-import": "^1.1.1",
    "babel-preset-react-app": "^2.1.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.20.0",
    "body-parser": "^1.17.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "css-loader": "0.26.1",
    "dotenv": "2.0.0",
    "eslint": "^3.8.1",
    "eslint-config-react-app": "^0.5.1",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "1.0.1",
    "filesize": "3.3.0",
    "file-loader": "0.10.0",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.24.0",
    "json-loader": "0.5.4",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "morgan": "^1.8.1",
    "node-sass": "^4.5.0",
    "postcss-loader": "1.2.2",
    "prettyjson": "^1.2.1",
    "prose-js": "0.0.8",
    "pseudo-js": "0.0.4",
    "react": "^15.4.2",
    "react-ace": "^4.1.6",
    "react-dev-utils": "^0.5.0",
    "react-dom": "^15.4.2",
    "react-materialize": "^0.18.1",
    "react-redux": "^5.0.2",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "react-typewriter": "^0.4.1",
    "recursive-readdir": "2.1.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.2",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "swagger-tools": "^0.10.1",
    "swagger-ui": "^3.0.9",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.2",
    "yamljs": "^0.2.10"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>\\config\\polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|dist|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>\\config\\jest\\cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>\\config\\jest\\fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
